.SUFFIXES : .cpp .$(OBJEXT) .exe
.cpp.$(OBJEXT) :
	$(CPP) $(CPPFLAGS) $(OFLAGS) $<


Borland:  \
	Dummy.exe

Microsoft:  \
	Dummy.exe

g++:  \
	Dummy.exe

edg:  \
	Dummy.exe

Metrowerks:  \
	Dummy.exe

clean:
ifeq ($(notdir $(SHELL)),COMMAND.COM)
	del *.obj
	del *.exe
	del *.tds
	del *.o
else
	rm -f *.o *.obj *.exe *.tds
endif


Dummy.exe: Dummy.cpp


